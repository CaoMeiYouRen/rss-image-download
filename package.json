{
  "name": "rss-image-download",
  "version": "0.1.0",
  "description": "自动从 RSS 下载图片，自动打包，自动备份",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint . --fix",
    "typecheck": "tsc --noEmit",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsdown",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "dev:tsdown": "cross-env NODE_ENV=development tsdown --watch",
    "start": "node ./dist/index.mjs",
    "start:tsx": "cross-env NODE_ENV=production tsx src/index.ts",
    "release": "semantic-release",
    "commit": "cz",
    "prepare": "husky",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/archiver": "^7.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.2.3",
    "commitizen": "^4.3.1",
    "commitlint": "^20.4.1",
    "commitlint-config-cmyr": "^1.0.0",
    "conventional-changelog-cmyr-config": "^3.0.0",
    "cross-env": "^10.0.0",
    "cz-conventional-changelog-cmyr": "^2.0.0",
    "debug": "^4.4.3",
    "eslint": "^9.34.0",
    "eslint-config-cmyr": "^2.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.3",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.0.5",
    "vitest": "^4.0.18"
  },
  "config": {},
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "rss",
    "image",
    "download",
    "backup",
    "typescript"
  ],
  "homepage": "https://github.com/CaoMeiYouRen/rss-image-download#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/rss-image-download.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/rss-image-download/issues"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "await-to-js": "^3.0.0",
    "axios": "^1.13.5",
    "axios-retry": "^4.5.0",
    "better-sqlite3": "^12.6.2",
    "cron": "^4.4.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.4",
    "fs-extra": "^11.3.3",
    "lodash-es": "^4.17.23",
    "log4js": "^6.9.1",
    "p-queue": "^9.1.0",
    "push-all-in-one": "^4.5.1",
    "rss-parser": "^3.13.0",
    "tslib": "^2.8.1",
    "yaml": "^2.8.2",
    "zx": "^8.8.5"
  },
  "pnpm": {
    "overrides": {
      "compare-func": "^2.0.0",
      "katex": "^0.16.21",
      "esbuild": "^0.25.0",
      "@octokit/plugin-paginate-rest": "^11.4.1",
      "@octokit/request": "^9.2.1",
      "@octokit/request-error": "^5.1.1",
      "brace-expansion": "^2.0.2",
      "tmp": "^0.2.4",
      "glob": "^11.1.0",
      "js-yaml": "^4.1.1"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  },
  "type": "module"
}